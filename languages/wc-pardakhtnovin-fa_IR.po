#, php-format
msgid "An error occurred while connecting to the payment gateway: <br/>%s"
msgstr "خطا در هنگام ارسال به درگاه پرداخت : %s"

#: index.php:364
msgid "Pay"
msgstr "پرداخت"

#: index.php:365
msgid "Back to checkout"
msgstr "بازگشت"

#: index.php:720
msgid "Connecting to payment gateway..."
msgstr "در حال اتصال به درگاه پرداخت..."

#: index.php:840
#, php-format
msgid ""
"Payment was successful.<br/>Transaction Tracking Code (Reference Code): %s "
"<br/> Transaction Request Number: %s"
msgstr ""
"پرداخت موفقیت آمیز بود .<br/> کد رهگیری (کد مرجع تراکنش) : %s <br/> شماره "
"درخواست تراکنش : %s"

